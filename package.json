{
  "name": "preppo-backend",
  "version": "1.0.0",
  "description": "Backend API for Preppo - Tabletop RPG GM Assistant",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "node -r ts-node/register migrations/run-migrations.ts",
    "migrate:create": "node -r ts-node/register migrations/create-migration.ts",
    "db:reset-dev": "node -r ts-node/register scripts/reset-dev-db.ts",
    "backfill-embeddings": "ts-node src/scripts/backfill-embeddings.ts",
    "backfill-embeddings:dry": "ts-node src/scripts/backfill-embeddings.ts --dry-run"
  },
  "keywords": [
    "rpg",
    "gm-assistant",
    "rag",
    "tabletop"
  ],
  "author": "Preppo Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/pdf-parse": "^1.1.5",
    "ajv": "^8.12.0",
    "aws-sdk": "^2.1500.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "file-type": "^16.5.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pg-boss": "^9.0.3",
    "pg-promise": "^11.5.4",
    "pgvector": "^0.1.4",
    "redis": "^4.6.11",
    "seedrandom": "^3.0.5",
    "tiktoken": "^1.0.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/seedrandom": "^3.0.8",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
